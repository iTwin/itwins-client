{
  "name": "@itwin/itwins-client",
  "version": "1.5.0",
  "description": "iTwins client for the iTwin platform",
  "main": "lib/cjs/itwins-client.js",
  "module": "lib/esm/itwins-client.js",
  "typings": "lib/cjs/itwins-client",
  "scripts": {
    "build": "npm run -s build:cjs && npm run -s build:esm",
    "build:cjs": "tsc 1>&2 --outDir lib/cjs",
    "build:esm": "tsc 1>&2 --module ES2020 --outDir lib/esm",
    "lint": "eslint -f visualstudio \"./src/**/*.ts\" 1>&2",
    "lint:fix": "npm run lint -- --fix",
    "webpackTests": "webpack --config ./src/test/utils/webpack.config.js 1>&2",
    "test": "npm run -s webpackTests && npm run -s test:chrome",
    "test:chrome": "certa -r chrome",
    "test:electron": "certa -r electron",
    "pack": "npm pack"
  },
  "keywords": [
    "Bentley",
    "iTwin",
    "digital-twin",
    "IModelHub",
    "project",
    "asset"
  ],
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/iTwin/itwins-client"
  },
  "dependencies": {
    "axios": "^1.7.7"
  },
  "devDependencies": {
    "@itwin/build-tools": "^3.8.0",
    "@itwin/certa": "^3.8.0",
    "@itwin/core-bentley": "^3.8.0",
    "@itwin/eslint-plugin": "^3.8.0",
    "@itwin/oidc-signin-tool": "~3.6.1",
    "@types/chai": "^4.3.20",
    "@types/mocha": "^8.2.3",
    "@types/node": "14.14.31",
    "chai": "^4.5.0",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "eslint": "^7.32.0",
    "mocha": "^8.4.0",
    "source-map-loader": "^1.1.3",
    "typescript": "~4.4.4",
    "webpack": "4.42.0",
    "webpack-cli": "^4.10.0"
  },
  "eslintConfig": {
    "plugins": [
      "@itwin"
    ],
    "extends": "plugin:@itwin/itwinjs-recommended",
    "rules": {
      "no-duplicate-imports": "off",
      "@typescript-eslint/consistent-type-imports": "error"
    }
  },
  "pnpm": {
    "overrides": {
      "nanoid@>=3.0.0 <3.1.31": ">=3.1.31",
      "minimatch@<3.0.5": ">=3.0.5",
      "braces@<3.0.3": ">=3.0.3",
      "ws@>=8.0.0 <8.17.1": ">=8.17.1",
      "semver@>=7.0.0 <7.5.2": ">=7.5.2",
      "micromatch@<4.0.8": ">=4.0.8"
    }
  }
}
